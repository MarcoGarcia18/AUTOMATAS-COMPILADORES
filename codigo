#include <iostream>
#include <string>
#include <sstream>
#include <cctype>
using namespace std;

string clasificar(const string &cadena) {
    bool tieneLetra = false;
    bool tieneNumero = false;
    bool otrosCaracteres = false;

    for (size_t i = 0; i < cadena.size(); ++i) {
        unsigned char uc = static_cast<unsigned char>(cadena[i]);
        if (isalpha(uc))
            tieneLetra = true;
        else if (isdigit(uc))
            tieneNumero = true;
        else
            otrosCaracteres = true;
    }

    if (tieneNumero && !tieneLetra && !otrosCaracteres)
        return "Numero entero";
    else if (tieneLetra && !tieneNumero && !otrosCaracteres)
        return "Palabra";
    else if (tieneLetra && tieneNumero && !otrosCaracteres)
        return "Compuesta";
    else
        return "No clasificada";
}

int main() {
    string linea;
    cout << "Escriba palabras o numeros (escriba 'salir' para terminar):\n";

    while (true) {
        cout << "> ";
        if (!getline(cin, linea)) break;   
        if (linea == "salir") break;

        stringstream ss(linea);
        string palabra;
        while (ss >> palabra) {
            cout << palabra << " -> " << clasificar(palabra) << '\n';
        }
    }
    return 0;
}
